.navbar.white.navbar-default(style='background-color:rgba(255,255,255,0.0)')
  .container
    .navbar-header
      button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand(href='/')
        i.fa.fa-cube
        | Playstore App
    .collapse.navbar-collapse
      ul.nav.navbar-nav
        li.white(class=(title == 'Home') ? 'active' : undefined)
          a(href='/') Home
        if user
          li.white(class=(title == 'Youtube') ? 'active' : undefined)
            a(href='/youjson') Youtube


