doctype html
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(href='https://fonts.googleapis.com/css?family=Roboto', rel='stylesheet')
    meta(name='theme-color' content='#4DA5F4')
    meta(name='csrf-token', content=_csrf)
    title #{title} - Reviews App
    link(rel='shortcut icon', href='/favicon.png')
    link(rel='stylesheet', href='/css/custom.css')
    link(rel='stylesheet', href='/css/simple-sidebar.css')
    link(rel='stylesheet', href='/css/main.css')
    link(rel='stylesheet', href='/css/demo_style.css')
    link(rel='stylesheet', href='/css/jquery.steps.css')
    script(src='/js/lib/jquery-2.2.4.min.js')
    link(rel='stylesheet', href='/css/smart_wizard.css')

     
    
    


  body

    

    
    
    .col-sm-12
        .col-sm-3
            include partials/header

        .col-sm-9
            a#menu-toggle.btn.btn-default.pull-right(href='#menu-toggle')
                i.fa.fa-bars
            include partials/flash
            block content

    include partials/footerpage
        
        

    
    script(src='/js/lib/bootstrap.min.js')
    script(src='/js/main.js')
